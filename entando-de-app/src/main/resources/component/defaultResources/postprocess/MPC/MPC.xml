<contentType>
	<defaultModelId>10001</defaultModelId>
	<listModelId>10011</listModelId>
	<typeCode>MPC</typeCode>
	<typeDescription>Multi-Purpose Content</typeDescription>
	<attributes>
		<attribute>
			<name>title</name>
			<description>Title</description>
			<type>Text</type>
			<roles>
				<role>jacms:title</role>
			</roles>
			<searchable>true</searchable>
			<indexable>true</indexable>
			<validationRules xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="textAttributeValidationRules">
				<required>true</required>
			</validationRules>
		</attribute>
		<attribute>
			<name>abstract</name>
			<description>Abstract</description>
			<type>Hypertext</type>
			<indexable>true</indexable>
		</attribute>
		<attribute xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="jaxbListAttributeType">
			<name>paragraphs</name>
			<description>Paragraphs</description>
			<type>Monolist</type>
			<nestedType xsi:type="jaxbCompositeAttributeType">
				<name>paragraphs</name>
				<type>Composite</type>
				<compositeElementTypes>
					<compositeElementType xsi:type="defaultJAXBAttributeType">
						<name>ptitle</name>
						<description>Title</description>
						<type>Text</type>
					</compositeElementType>
					<compositeElementType xsi:type="defaultJAXBAttributeType">
						<name>pmain</name>
						<description>Main Content</description>
						<type>Hypertext</type>
					</compositeElementType>
					<compositeElementType xsi:type="defaultJAXBAttributeType">
						<name>pimage</name>
						<description>Image</description>
						<type>Image</type>
					</compositeElementType>
					<compositeElementType xsi:type="defaultJAXBAttributeType">
						<name>pcaption</name>
						<description>Image Caption</description>
						<type>Text</type>
					</compositeElementType>
				</compositeElementTypes>
			</nestedType>
		</attribute>
		<attribute xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="jaxbListAttributeType">
			<name>links</name>
			<description>Links</description>
			<type>Monolist</type>
			<nestedType>
				<name>Links</name>
				<type>Link</type>
				<indexable>true</indexable>
			</nestedType>
		</attribute>
		<attribute xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="jaxbListAttributeType">
			<name>attaches</name>
			<description>Attachments</description>
			<type>Monolist</type>
			<nestedType>
				<name>Attaches</name>
				<type>Attach</type>
				<indexable>true</indexable>
			</nestedType>
		</attribute>
	</attributes>
</contentType>
